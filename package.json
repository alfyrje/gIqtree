{
  "name": "gIqtree",
  "version": "0.1.0",
  "private": true,
  "description": "iqtree",
  "author": "iqtree",
  "build": {
    "appId": "com.giqtree",
    "mac": {
      "category": "your.app.category.type",
      "icon": "build/icon.png"
    },
    "win": {
      "icon": "build/icon.png"
    }
  },
  "main": "./build/electron.js",
  "homepage": "./",
  "scripts": {
    "test": "jest",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "watch-electron": "tsc -p electron -w",
    "build-ts-electron": "tsc --incremental false --tsBuildInfoFile null -p electron",
    "build": "SKIP_PREFLIGHT_CHECK=true yarn react-build && yarn build-ts-electron && yarn electron-build --win portable -l",
    "build:win": "SKIP_PREFLIGHT_CHECK=true yarn react-build && yarn build-ts-electron && yarn electron-build --win portable",
    "build:linux": "SKIP_PREFLIGHT_CHECK=true yarn react-build && yarn build-ts-electron && yarn electron-build -l",
    "build:mac": "SKIP_PREFLIGHT_CHECK=true yarn react-build && yarn build-ts-electron && yarn electron-build -m",
    "start": "concurrently \"cross-env HOST=localhost SKIP_PREFLIGHT_CHECK=true BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && yarn build-ts-electron && electron build/electron.js\""
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@giap/phylotree": "^1.1.3",
    "@material-ui/core": "^4.12.2",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.26.0",
    "clsx": "^1.1.1",
    "core-js": "^3.19.0",
    "cross-env": "^7.0.3",
    "decompress": "^4.2.1",
    "electron-is-dev": "^2.0.0",
    "fs-extra": "^10.0.0",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-d3-tree": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-rnd": "^10.3.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll-to-bottom": "^4.2.0",
    "sqlite3": "^5.0.2",
    "tree-kill": "^1.2.2",
    "unzipper": "^0.10.11",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "14",
    "concurrently": "^6.2.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "test": "^0.6.0",
    "typescript": "^4.6.2",
    "wait-on": "^6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
